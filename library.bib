  journaltitle = {Genes and Development},
  keywords = {Histone H2A.Z,Histone H3.3,Nucleosome structure},
  number = {12},
  pages = {1519-1529},
  title = {Nucleosome Stability Mediated by Histone Variants {{H3}}.3 and {{H2A}}.{{Z}}},
  volume = {21}
}

