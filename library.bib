  issn = {14712164},
  journaltitle = {BMC Genomics},
  number = {1},
  title = {Combined {{ChIP}}-{{Seq}} and Transcriptome Analysis Identifies {{AP}}-1/{{JunD}} as a Primary Regulator of Oxidative Stress and {{IL}}-1{$\beta$} Synthesis in Macrophages},
