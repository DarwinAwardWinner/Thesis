  doi = {10/c9r6nn},
  file = {/Users/ryan/Documents/Zotero Library/Berman et al. - 2010 - Mesenchymal stem cells enhance allogeneic islet en.pdf},
  issn = {00121797},
  journaltitle = {Diabetes},
