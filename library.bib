  keywords = {DNA methylation,pancreatic islets,type 2 diabetes},
  number = {6},
  pages = {1405-1426},
  title = {{{DNA}} Methylation Profiling Identifies Epigenetic Dysregulation in Pancreatic Islets from Type 2 Diabetic Patients},
