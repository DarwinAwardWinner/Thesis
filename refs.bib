pages = {e67019},
pmid = {23843979},
title = {{ANOVA-like differential expression (ALDEx) analysis for mixed population RNA-Seq.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3699591{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {8},
year = {2013}
}
