file = {:Users/ryan/Documents/Mendeley Desktop/Kikutake, Yahara - 2016 - Identification of epigenetic biomarkers of lung adenocarcinoma through multi-omics data analysis.pdf:pdf},
issn = {19326203},
journal = {PLoS ONE},
