isbn = {1438-7948 (Electronic)$\backslash$r1438-793X (Linking)},
issn = {1438793X},
journal = {Functional and Integrative Genomics},
keywords = {Bovine,Butyrate,ChIP-seq,Epigenomics,Histone acetylation},
